    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "liquidjs": "^10.8.4",
    "lucide-react": "^0.511.0",
    "marked": "^4.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.6.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "turndown": "^7.1.1",
    "tw-animate-css": "^1.3.2"
